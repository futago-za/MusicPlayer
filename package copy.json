{
  "name": "musicplayer",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean lint build serve",
    "web": "react-scripts start",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p electron",
    "dev:webpack": "webpack --watch --progress",
    "predev": "rimraf dist",
    "clean": "rimraf dist release",
    "serve": "electron .",
    "build": "cross-env NODE_ENV=\"development\" webpack --config webpack.config.prod.ts --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.ts --progress",
    "build:pack": "ts-node ./build.ts",
    "package": "run-s clean build:prod build:pack",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/futago-za/musicplayer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/futago-za/musicplayer/issues"
  },
  "homepage": "https://gitlab.com/futago-za/musicplayer#readme",
  "devDependencies": {
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-search-devtools": "^1.5.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "ifdef-loader": "^2.3.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^2.4.4",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  }
}

{
   "name": "musicplayer",
   "version": "1.0.0",
   "description": "",
   "main": "dist/main.js",
   "scripts": {
      "start": "run-s clean lint build serve",
      "dev": "run-p dev:*",
      "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
      "dev:tsc": "tsc -w -p electron",
      "dev:webpack": "webpack --watch --progress",
      "predev": "rimraf dist",
      "clean": "rimraf dist release",
      "serve": "electron .",
      "build": "cross-env NODE_ENV=\"development\" webpack --config webpack.config.prod.ts --progress",
      "build:prod": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.ts --progress",
      "build:pack": "ts-node ./build.ts",
      "package": "run-s clean build:prod build:pack",
      "lint": "run-s lint:*",
      "lint:eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
      "lint:prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+ssh://git@gitlab.com/futago-za/musicplayer.git"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://gitlab.com/futago-za/musicplayer/issues"
   },
   "homepage": "https://gitlab.com/futago-za/musicplayer#readme",
   "devDependencies": {
      "@types/mini-css-extract-plugin": "^2.4.0",
      "@types/node": "^16.11.10",
      "@types/raf": "^3.4.0",
      "@types/react": "^17.0.34",
      "@types/react-dom": "^17.0.11",
      "@types/react-howler": "^3.7.3",
      "@types/react-router-dom": "^5.3.2",
      "@types/redux-localstorage": "^1.0.8",
      "@types/redux-thunk": "^2.1.0",
      "@typescript-eslint/eslint-plugin": "^5.3.0",
      "@typescript-eslint/parser": "^5.3.0",
      "cross-env": "^7.0.3",
      "css-loader": "^6.5.1",
      "electron": "^15.3.0",
      "electron-builder": "^22.13.1",
      "electron-reload": "^2.0.0-alpha.1",
      "electron-search-devtools": "^1.5.0",
      "eslint": "^7.16.0",
      "eslint-config-prettier": "^7.1.0",
      "html-webpack-plugin": "^5.5.0",
      "ifdef-loader": "^2.3.0",
      "lint-staged": "^10.5.3",
      "mini-css-extract-plugin": "^2.4.4",
      "node-sass": "^6.0.1",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.1",
      "rimraf": "^3.0.2",
      "sass-loader": "^12.3.0",
      "ts-loader": "^9.2.6",
      "ts-node": "^10.4.0",
      "typescript": "^4.4.4",
      "wait-on": "^6.0.0",
      "webpack": "^5.62.1",
      "webpack-cli": "^4.9.1",
      "webpack-dev-server": "^4.4.0"
   },
   "dependencies": {
      "@emotion/react": "^11.5.0",
      "@emotion/styled": "^11.3.0",
      "@mui/icons-material": "^5.1.0",
      "@mui/material": "^5.1.0",
      "@mui/styles": "^5.2.3",
      "@rocka/mp3-duration": "^2.0.0",
      "mp3-duration": "^1.1.0",
      "node-id3": "^0.2.3",
      "raf": "^3.4.1",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-howler": "^5.2.0",
      "react-redux": "^7.2.6",
